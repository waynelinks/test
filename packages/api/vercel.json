{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "PATCH",
              "DELETE"
          ]
      }
  ]
}
